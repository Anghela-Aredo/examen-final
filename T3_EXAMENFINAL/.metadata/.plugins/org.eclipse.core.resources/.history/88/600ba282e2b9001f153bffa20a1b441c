CREATE TABLE client (
    codigo_Unico VARCHAR(250) PRIMARY KEY,
    nombre VARCHAR(100) NOT NULL,
    apellido VARCHAR(100) NOT NULL,
    tipo_Documento VARCHAR(50) NOT NULL,
    numero_Documento VARCHAR(50) NOT NULL
);

